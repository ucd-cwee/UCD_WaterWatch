      <Command Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
	echo Invoking SWIG...
	echo on
	echo Deleting previously generated files...
	del /s/q "..\WaterWatch_Cs_SWIG\*.cs"
	echo Copying necessary Cpp files to local directory and renaming...
	del /s/q "$(ProjectDir)SWIG.cpp"
	del /s/q "$(ProjectDir)Header.h"
	copy /y "$(SolutionDir)WaterWatch_Cs_SWIG\impl.cpp" "$(ProjectDir)SWIG.cpp"
	copy /y "$(SolutionDir)WaterWatch_Cs_SWIG\Header.h" "$(ProjectDir)Header.h"
	echo Performing SWIG conversion to R programming language...
	"$(SolutionDir)swigwin-4.1.1\swig.exe" -c++ -r -o "$(ProjectDir)SWIG_wrap.cpp" "%(FullPath)" 
	echo Compiling new library using the R executable...
	"$(ProjectDir)R-4.3.1\bin\x64\R.exe" CMD SHLIB "$(ProjectDir)SWIG_wrap.cpp" "$(ProjectDir)SWIG.cpp"
	echo Finishing...
	%40echo off
      </Command>
      <Outputs Condition="'$(Configuration)|$(Platform)'=='Release|x64'">%(Filename)_wrap.cpp;$(ProjectDir)</Outputs>
    </CustomBuild>
